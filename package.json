{
  "name": "micro-video",
  "version": "1.0.0",
  "description": "a short video application develope on the framework React + TypeScript ",
  "main": "./src/index.tsx",
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.7.4",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "log-symbols": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^4.3.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./build/index.js",
    "build": "cross-env NODE_ENV=production node ./build/index.js",
    "build:dll": "webpack --progress --config ./build/webpack.dll.js",
    "lint": "eslint --fix --ext .ts,.tsx src",
    "serve": "http-server dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,.tsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --ignore-path .eslintignore",
      "git add "
    ],
    "src/**/*.{css,scss}": [
      "stylelint --config src/stylelint.config.json"
    ]
  },
  "keywords": [
    "scaffold",
    "typescript",
    "react",
    "micro-video",
    "video"
  ],
  "author": "harhao",
  "license": "MIT"
}
